<form method="post" action="/teams">
  <h1>Create a Team and Heroes!</h1>
  <label>Team Name: <input type="text" name="team[name]"></label>
  <label>Team Motto: <input type="text" name="team[motto]"></label>

  <h2>Hero 1</h2>
  <label>Hero's Name: <input type="text" name="team[members][][name]" id="member1_name"></label>
  <label>Hero's Power: <input type="text" name="team[members][][power]" id="member1_power"></label>
  <label>Hero's Biography: <input type="text" name="team[members][][bio]" id="member1_bio"></label>

  <h2>Hero 2</h2>
  <label>Hero's Name: <input type="text" name="team[members][][name]" id="member2_name"></label>
  <label>Hero's Power: <input type="text" name="team[members][][power]" id="member2_power"></label>
  <label>Hero's Biography: <input type="text" name="team[members][][bio]" id="member2_bio"></label>

  <h2>Hero 3</h2>
  <label>Hero's Name: <input type="text" name="team[members][][name]" id="member3_name"></label>
  <label>Hero's Power: <input type="text" name="team[members][][power]" id="member3_power"></label>
  <label>Hero's Biography: <input type="text" name="team[members][][bio]" id="member3_bio"></label>

  <button type="submit" id="submit">
</form>